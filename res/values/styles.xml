<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
	<style name="AutoCompleteTextViewOrangetheme" parent="android:Widget.Holo.Light.AutoCompleteTextView">
		<item name="android:dropDownSelector">@drawable/orangetheme_list_selector_holo_light</item>
		<item name="android:background">@drawable/orangetheme_edit_text_holo_light</item>
	</style>
	
	<style name="ButtonOrangetheme" parent="android:Widget.Holo.Light.Button">
		<item name="android:background">@drawable/orangetheme_btn_default_holo_light</item>
	</style>
	
	<style name="ImageButtonOrangetheme" parent="android:Widget.Holo.Light.ImageButton">
		<item name="android:background">@drawable/orangetheme_btn_default_holo_light</item>
	</style>
	
	<style name="MenuTextView">
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@android:color/black</item>
		<item name="android:typeface">sans</item>
	</style>
	
	<style name="DialogTitleStyle">
		<item name="android:background">@drawable/bottom_line</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
		<item name="android:textColor">@color/orange_main</item>
		<item name="android:typeface">sans</item>
		<item name="android:paddingLeft">24dp</item>
		<item name="android:paddingBottom">4dp</item>
		<item name="android:minHeight">?android:attr/listPreferredItemHeight</item>
	</style>
</resources>
